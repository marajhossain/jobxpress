{% extends 'base.html.twig' %}
{# START: Head #}
{% block meta %}
     {{  parent()  }}

	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <meta name="description" content="JobXpress is Open-Source job board software">
    <meta name="keywords" content="jobxpress, search job, post job">

	<link rel="apple-touch-icon" href="{{ asset('admin/images/ico/apple-icon-120.png') }}">
    <link rel="shortcut icon" type="image/x-icon" href="{{ asset('admin/images/ico/favicon.ico') }}">

{% endblock %}

{% block title %} Job Xpress {% endblock %}

{% block topjsscripts %}
    {{  parent()  }}
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i%7CQuicksand:300,400,500,700" rel="stylesheet">
{% endblock %}

{% block stylesheets %}
    {{  parent()  }}
	
	{# BEGIN: Bootstrap CSS #}
	<link rel="stylesheet" type="text/css" href="{{ asset('front/css/bootstrap.min.css') }}">
	{# END: Bootstrap CSS #}

	{# BEGIN: Custom CSS #}
	<link rel="stylesheet" type="text/css" href="{{ asset('front/css/style.css') }}">
	{# END: Custom CSS #}
{% endblock %}
{# END: Head #}

{# BEGIN: Header #}
{% block header %}
	{{  include('front/default/_header.html.twig')  }}
{% endblock %}
{# END: Header #}

{# BEGIN: Body #}
{% block body %}
	{# START: Content #}
	{% block content %} {% endblock %}
	{# END: Content #}
{% endblock %}
{# END: Body #}

{# BEGIN: Footer #}
{% block footer %}
	{{  include('front/default/_footer.html.twig')  }}
{% endblock %}
{# END: Footer #}

{# BEGIN: JavScripts #}
{% block javascripts %}
	{# BEGIN: Page JS #}
	<script src="{{ asset('front/js/jquery-3.5.1.slim.min.js') }}"></script>
	<script src="{{ asset('front/js/bootstrap.bundle.min.js') }}"></script>
	<script src="{{ asset('front/js/bootstrap.min.js') }}"></script>
	{# END: Page JS #}
{% endblock %} 
{# END: JavScripts #}

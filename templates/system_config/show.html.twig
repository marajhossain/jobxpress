{% extends 'base.html.twig' %}

{% block title %}SystemConfig{% endblock %}

{% block body %}
    <div class="col-lg-12">
        <h1 class="page-header">System Config View</h1>
    </div>

    <div class="col-lg-12">
        <table class="table">
            <tbody>
                <tr>
                    <th>Id</th>
                    <td>{{ system_config.id }}</td>
                </tr>
                <tr>
                    <th>Name</th>
                    <td>{{ system_config.name }}</td>
                </tr>
                <tr>
                    <th>Config_name</th>
                    <td>{{ system_config.configName }}</td>
                </tr>
                <tr>
                    <th>Config_value</th>
                    <td>{{ system_config.configValue }}</td>
                </tr>
            </tbody>
        </table>
    </div>

    <a href="{{ path('system_config_index') }}" class="text-danger"> <i class="fa fa-arrow-left"></i> back to list</a>

{% endblock %}
